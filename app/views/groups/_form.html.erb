<% form_for(@group) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <% @users.each do |user| %>
    <p>
      <label><%=h user.name %></label>
      <%= check_box_tag :"users[]", user.id, @group.users.include?(user) %>
    </p>
  <% end %>
  <p>
    <%= f.submit 'Submit' %>
  </p>
<% end %>